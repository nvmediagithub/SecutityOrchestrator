<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_14"
                  targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="Process_14_product_application" isExecutable="false">
    <bpmn:startEvent id="StartEvent_14" name="Клиент решает подать заявку">
      <bpmn:outgoing>Flow_14_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_14_Auth" name="Аутентификация: POST /auth/bank-token">
      <bpmn:incoming>Flow_14_1</bpmn:incoming>
      <bpmn:outgoing>Flow_14_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_14_GetProd" name="Получение списка продуктов: GET /products">
      <bpmn:incoming>Flow_14_2</bpmn:incoming>
      <bpmn:outgoing>Flow_14_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_14_Apply" name="Подача заявки: POST /product-application">
      <bpmn:incoming>Flow_14_3</bpmn:incoming>
      <bpmn:outgoing>Flow_14_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_14" name="Заявка подана">
      <bpmn:incoming>Flow_14_4</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_14_1" sourceRef="StartEvent_14" targetRef="Task_14_Auth"/>
    <bpmn:sequenceFlow id="Flow_14_2" sourceRef="Task_14_Auth" targetRef="Task_14_GetProd"/>
    <bpmn:sequenceFlow id="Flow_14_3" sourceRef="Task_14_GetProd" targetRef="Task_14_Apply"/>
    <bpmn:sequenceFlow id="Flow_14_4" sourceRef="Task_14_Apply" targetRef="EndEvent_14"/>
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_14">
    <bpmndi:BPMNPlane id="BPMNPlane_14" bpmnElement="Process_14_product_application">
      <bpmndi:BPMNShape id="StartEvent_14_di" bpmnElement="StartEvent_14">
        <dc:Bounds x="100" y="200" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_14_Auth_di" bpmnElement="Task_14_Auth">
        <dc:Bounds x="200" y="188" width="100" height="60"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_14_GetProd_di" bpmnElement="Task_14_GetProd">
        <dc:Bounds x="350" y="188" width="100" height="60"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_14_Apply_di" bpmnElement="Task_14_Apply">
        <dc:Bounds x="500" y="188" width="100" height="60"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_14_di" bpmnElement="EndEvent_14">
        <dc:Bounds x="650" y="200" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="Flow_14_1_di" bpmnElement="Flow_14_1">
        <di:waypoint x="136" y="218"/>
        <di:waypoint x="200" y="218"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_14_2_di" bpmnElement="Flow_14_2">
        <di:waypoint x="300" y="218"/>
        <di:waypoint x="350" y="218"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_14_3_di" bpmnElement="Flow_14_3">
        <di:waypoint x="450" y="218"/>
        <di:waypoint x="500" y="218"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_14_4_di" bpmnElement="Flow_14_4">
        <di:waypoint x="600" y="218"/>
        <di:waypoint x="650" y="218"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>