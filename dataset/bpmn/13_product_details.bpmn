      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_13_di" bpmnElement="EndEvent_13">
        <dc:Bounds x="350" y="200" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="Flow_13_1_di" bpmnElement="Flow_13_1">
        <di:waypoint x="136" y="218"/>
        <di:waypoint x="200" y="218"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_13_2_di" bpmnElement="Flow_13_2">
        <di:waypoint x="300" y="218"/>
        <di:waypoint x="350" y="218"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
