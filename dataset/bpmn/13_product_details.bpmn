      <bpmn:outgoing>Flow_13_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_13_GetProdDet" name="Получение деталей продукта: GET /products/{product_id}">
      <bpmn:incoming>Flow_13_1</bpmn:incoming>
      <bpmn:outgoing>Flow_13_2</bpmn:outgoing>
