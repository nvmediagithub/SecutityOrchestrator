      <bpmn:outgoing>Flow_09_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_09_CreateLead" name="Создание лида: POST /customer-leads">
      <bpmn:incoming>Flow_09_1</bpmn:incoming>
