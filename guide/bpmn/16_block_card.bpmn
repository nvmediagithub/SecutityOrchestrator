<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_16"
                  targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="Process_16_block_card" isExecutable="false">
    <bpmn:startEvent id="StartEvent_16" name="Клиент теряет карту">
      <bpmn:outgoing>Flow_16_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_16_Auth" name="Аутентификация: POST /auth/bank-token">
      <bpmn:incoming>Flow_16_1</bpmn:incoming>
      <bpmn:outgoing>Flow_16_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_16_GetAccs" name="Получение списка счетов: GET /accounts">
      <bpmn:incoming>Flow_16_2</bpmn:incoming>
      <bpmn:outgoing>Flow_16_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_16_CloseAcc" name="Закрытие счёта: PUT /accounts/{account_id}/status (status: closed)">
      <bpmn:incoming>Flow_16_3</bpmn:incoming>
      <bpmn:outgoing>Flow_16_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_16" name="Счёт карты закрыт (заблокирован)">
      <bpmn:incoming>Flow_16_4</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_16_1" sourceRef="StartEvent_16" targetRef="Task_16_Auth"/>
    <bpmn:sequenceFlow id="Flow_16_2" sourceRef="Task_16_Auth" targetRef="Task_16_GetAccs"/>
    <bpmn:sequenceFlow id="Flow_16_3" sourceRef="Task_16_GetAccs" targetRef="Task_16_CloseAcc"/>
    <bpmn:sequenceFlow id="Flow_16_4" sourceRef="Task_16_CloseAcc" targetRef="EndEvent_16"/>
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_16">
    <bpmndi:BPMNPlane id="BPMNPlane_16" bpmnElement="Process_16_block_card">
      <bpmndi:BPMNShape id="StartEvent_16_di" bpmnElement="StartEvent_16">
        <dc:Bounds x="100" y="200" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_16_Auth_di" bpmnElement="Task_16_Auth">
        <dc:Bounds x="200" y="188" width="100" height="60"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_16_GetAccs_di" bpmnElement="Task_16_GetAccs">
        <dc:Bounds x="350" y="188" width="100" height="60"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_16_CloseAcc_di" bpmnElement="Task_16_CloseAcc">
        <dc:Bounds x="500" y="188" width="100" height="60"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_16_di" bpmnElement="EndEvent_16">
        <dc:Bounds x="650" y="200" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="Flow_16_1_di" bpmnElement="Flow_16_1">
        <di:waypoint x="136" y="218"/>
        <di:waypoint x="200" y="218"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_16_2_di" bpmnElement="Flow_16_2">
        <di:waypoint x="300" y="218"/>
        <di:waypoint x="350" y="218"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_16_3_di" bpmnElement="Flow_16_3">
        <di:waypoint x="450" y="218"/>
        <di:waypoint x="500" y="218"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_16_4_di" bpmnElement="Flow_16_4">
        <di:waypoint x="600" y="218"/>
        <di:waypoint x="650" y="218"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>