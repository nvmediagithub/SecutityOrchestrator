package org.example.infrastructure.services.bpmn;

import java.util.List;
import java.util.ArrayList;

/**
 * Flow analysis result for BPMN processes
 */
public class FlowAnalysisResult {
    private List<String> deadPaths = new ArrayList<>();
    private List<String> unreachableTasks = new ArrayList<>();
    private List<String> missingGateways = new ArrayList<>();
    private List<String> inconsistentSequenceFlows = new ArrayList<>();
    private int totalFlows;
    private int validFlows;
    private int invalidFlows;
    private double flowComplexity;
    private String complexityLevel;

    public FlowAnalysisResult() {}

    public List<String> getDeadPaths() {
        return deadPaths;
    }

    public void setDeadPaths(List<String> deadPaths) {
        this.deadPaths = deadPaths;
    }

    public List<String> getUnreachableTasks() {
        return unreachableTasks;
    }

    public void setUnreachableTasks(List<String> unreachableTasks) {
        this.unreachableTasks = unreachableTasks;
    }

    public List<String> getMissingGateways() {
        return missingGateways;
    }

    public void setMissingGateways(List<String> missingGateways) {
        this.missingGateways = missingGateways;
    }

    public List<String> getInconsistentSequenceFlows() {
        return inconsistentSequenceFlows;
    }

    public void setInconsistentSequenceFlows(List<String> inconsistentSequenceFlows) {
        this.inconsistentSequenceFlows = inconsistentSequenceFlows;
    }

    public int getTotalFlows() {
        return totalFlows;
    }

    public void setTotalFlows(int totalFlows) {
        this.totalFlows = totalFlows;
    }

    public int getValidFlows() {
        return validFlows;
    }

    public void setValidFlows(int validFlows) {
        this.validFlows = validFlows;
    }

    public int getInvalidFlows() {
        return invalidFlows;
    }

    public void setInvalidFlows(int invalidFlows) {
        this.invalidFlows = invalidFlows;
    }

    public double getFlowComplexity() {
        return flowComplexity;
    }

    public void setFlowComplexity(double flowComplexity) {
        this.flowComplexity = flowComplexity;
    }

    public String getComplexityLevel() {
        return complexityLevel;
    }

    public void setComplexityLevel(String complexityLevel) {
        this.complexityLevel = complexityLevel;
    }
}